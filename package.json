{
  "name": "ember-codemod-remove-inject-as-service",
  "version": "2.3.1",
  "description": "Codemod to remove `inject as service`",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-codemod-remove-inject-as-service.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/ember-codemod-remove-inject-as-service.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "sh build.sh --production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm build",
    "release:prepare": "changeset version",
    "release:publish": "pnpm build && changeset publish",
    "test": "sh build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/ast-javascript": "^2.1.4",
    "@codemod-utils/ast-template-tag": "^1.2.2",
    "@codemod-utils/files": "^3.2.5",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@codemod-utils/tests": "^2.2.4",
    "@ijlee2-frontend-configs/changesets": "^1.0.2",
    "@ijlee2-frontend-configs/eslint-config-node": "^2.4.1",
    "@ijlee2-frontend-configs/prettier": "^2.4.1",
    "@sondr3/minitest": "^0.1.2",
    "@tsconfig/node20": "^20.1.9",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^20.19.31",
    "@types/yargs": "^17.0.35",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": "20.* || >= 22"
  }
}
